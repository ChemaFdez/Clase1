    "\n",
    "matriz = ([2,3],[3,4])\n",
    "def sumadiagonal(matriz):\n",
    "    suma = 0\n",
    "    for i in range(len(matriz[0])): \n",
    "        for j in range(len(matriz[0])):\n",
    "            if i == j:\n",
    "                suma = suma + matriz[i][j]\n",
    "    return suma\n",
    "print(\"La suma es\" ,sumadiagonal(matriz))\n",
